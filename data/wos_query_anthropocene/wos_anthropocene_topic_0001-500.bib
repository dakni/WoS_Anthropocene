@article{ ISI:000395609300009,
Author = {Hobbs, William O. and Edlund, Mark B. and Umbanhowar, Jr., Charles E.
   and Camill, Philip and Lynch, Jason A. and Geiss, Christoph and
   Stefanova, Vania},
Title = {{Holocene evolution of lakes in the forest-tundra biome of northern
   Manitoba, Canada}},
