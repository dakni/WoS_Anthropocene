@article{ ISI:000400908900003,
Author = {Colebrook, Claire Mary},
Title = {{Sex and the (Anthropocene) City}},
Journal = {{THEORY CULTURE \& SOCIETY}},
Year = {{2017}},
Volume = {{34}},
Number = {{2-3, SI}},
Pages = {{39-60}},
Month = {{MAR-MAY}},
Abstract = {{In this essay I explore three concepts: sex, the city, and the
   Anthropocene. I argue that the condition for the possibility of the city
   is the assemblage of sexual drives for the sake of relative stability,
   but that those same drives also exceed the city's self-preservative
   function. Further, I argue that the very conditions that further the
   city and that enable philosophical and scientific concepts to be formed
   (and that allow for the Anthropocene to be discerned as an epoch) rely
   upon a geological politics that enables new ways of thinking about what
   counts as the political as such.}},
