@article{ ISI:000427477600017,
Author = {Obregon, C. and Lyndon, A. R. and Barker, J. and Christiansen, H. and
   Godley, B. J. and Kurland, S. and Piccolo, J. J. and Potts, R. and
   Short, R. and Tebb, A. and Mariani, S.},
Title = {{Valuing and understanding fish populations in the Anthropocene: key
   questions to address}},
Journal = {{JOURNAL OF FISH BIOLOGY}},
Year = {{2018}},
