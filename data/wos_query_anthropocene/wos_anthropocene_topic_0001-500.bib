@article{ ISI:000418222200012,
Author = {Dijkstra, Noortje and Junttila, Juho and Aagaard-Sorensen, Steffen},
Title = {{Environmental baselines and reconstruction of Atlantic Water inflow in
   Bjornoyrenna, SW Barents Sea, since 1800 CE}},
Journal = {{MARINE ENVIRONMENTAL RESEARCH}},
Year = {{2017}},
Volume = {{132}},
Pages = {{117-131}},
Month = {{DEC}},
Abstract = {{Metal concentrations, sediment properties and benthic foraminiferal
   assemblages were investigated in sediment cores in the SW Barents Sea,
   to reconstruct environmental baselines and natural variability of
   Atlantic Water inflow since 1800 CE. Metal concentrations correspond to
   no effect levels and do not influence the foraminifera. Increased Hg and
   Pb was linked to inflow of Atlantic Water. The data set is considered to
   reflect the pre-impacted environmental baseline and range in natural
   variability of the study area. The foraminiferal assemblages in the SW
   part of the study area showed warming and presence of Atlantic Water
   towards 1900 CE. The NE part of the region indicate presence of cold
   Artic Water influenced conditions. Between 1900 and 1980 CE, the SW
   region indicates reduced inflow of Atlantic Water. From 1980 CE towards
   the present the assemblages of the entire study area show warming of
   Atlantic Water and northward retreat of the Arctic Front.}},
