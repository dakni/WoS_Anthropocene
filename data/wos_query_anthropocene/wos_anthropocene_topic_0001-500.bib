@article{ ISI:000406115000005,
Author = {Wen, Jun and Harris, A. J. and Ickert-Bond, Stefanie M. and Dikow,
   Rebecca and Wurdack, Kenneth and Zimmer, Elizabeth A.},
Title = {{Developing integrative systematics in the informatics and genomic era,
   and calling for a global Biodiversity Cyberbank}},
Journal = {{JOURNAL OF SYSTEMATICS AND EVOLUTION}},
Year = {{2017}},
Volume = {{55}},
Number = {{4, SI}},
