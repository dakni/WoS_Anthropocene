@article{ ISI:000419238500007,
Author = {Barnard, John Levi},
Title = {{The Cod and the Whale: Melville in the Time of Extinction}},
Journal = {{AMERICAN LITERATURE}},
Year = {{2017}},
Volume = {{89}},
Number = {{4}},
Pages = {{851-879}},
Month = {{DEC}},
Abstract = {{This essay tracks the interrelated histories of the cod and the whale
   from John Smith's colonial American vision of abundance to the near
   disappearance of the living resources he described as inexhaustible.
   Working at the intersections of animal studies, food studies, and the
   environmental humanities-and considering cultural artifacts from Smith's
   tracts and Herman Melville's fiction to Cheryl Savageau's poetry and the
   underwater sculpture of Jason deCaires Taylor-the essay reads the
   commodification and consumption of these creatures as constitutive
   elements of an extinction-producing economy. Within this reading, the
   cod and the whale appear as exemplary figures not only for the important
   distinction between the edible and the exotic animal-wild food and
   wildlife-but also for the larger historical relation between ``animal
   capital{''} and mass extinction.}},
