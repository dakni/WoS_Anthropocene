@article{ ISI:000418424200001,
Author = {Bohle, Martin and Sibilla, Anna and Casals i Graells, Robert},
Title = {{A Concept of Society-Earth-Centric Narratives}},
Journal = {{ANNALS OF GEOPHYSICS}},
Year = {{2017}},
Volume = {{60}},
Number = {{7}},
Abstract = {{This essay presents a concept of narratives as powerful means for people
   to associate themselves with anthropogenic global change. The starting
   point of our reflection is that people's awareness is related to their
   story-telling practices to communicate insights and to induce or inspire
   behaviour. By analysing traditional and modern settings of
