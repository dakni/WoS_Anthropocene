@article{ ISI:000424894300003,
Author = {Kalshoven, Petra Tjitske},
Title = {{Gestures of taxidermy: Morphological approximation as interspecies
   affinity}},
Journal = {{AMERICAN ETHNOLOGIST}},
Year = {{2018}},
Volume = {{45}},
Number = {{1}},
Pages = {{34-47}},
Month = {{FEB}},
Abstract = {{Anthropologists have recently begun to highlight human relatedness with
   other animal species, arguing for a more inclusive posthumanism in which
   boundaries between different categories of life become blurred.
   Taxidermy in Britain and western Europe both troubles and supports
   assumptions about interspecies entanglements. In taxidermy, living
   humans meet dead animals in ways that suggest kinship relations beyond
   death, expressed in morphological analogies. Lifelike animation occurs
   both discursively and plastically, and the recent influx of artists into
   taxidermy has given it particular prominence. A specific ethics of the
   body emerges, one that makes a professed environmental affinity among
   artist-taxidermists pale in comparison with the morphological
   approximation performed by professional taxidermists in relation to the
   animals whose lives they claim to prolong. {[}morphology, ethics of the
   body, taxidermy, interspecies affinity, morphological approximation,
   Britain, western Europe]}},
