@article{ ISI:000418507400003,
Author = {Wang, Yiman},
Title = {{The mortal animal and the techno-animated life - Chen Qiang's
   experimental zooetic animation trilogy, Cut off}},
Journal = {{JOURNAL OF CHINESE CINEMAS}},
Year = {{2017}},
Volume = {{11}},
Number = {{3}},
Pages = {{210-226}},
Abstract = {{This article revisits animation's commonly recognized life-endowing
   techniques via the example of a stop-motion animation trilogy, Cut off
   (2007, 2008, 2010), by a Chinese experimental artist, Chen Qiang, with
   the goal of channeling the vitalist principle of animation toward an
   eco-cinematic concern. It unpacks the ways in which Chen's
   labor-intensive stopmotion animation of the `food actors' and pixilation
   of the human actors (mixed with live action) draw upon the Czech New
   Wave artist, Jan Svankmajer's surrealist approach to produce posthumous
   techno-life for the delinquent animal parts and jettisoned vegetables
   and fruits. My study demonstrates that the trilogy pushes Una
   Chaudhuri's notion of zooesis one step further to not only take animals
   seriously as material entities, but also re-enchant what is
   conventionally considered rotten and inanimate with renewed
   technologized life forms. Building upon the basis of the case-specific
   `microanalysis', I argue that the trilogy forces us to reckon with
   abject life-hood and agency, to resist the capital-driven Anthropocene,
   thereby to ponder strategies of building a bio-diverse, equitable and
   sustainable ecological community.}},
