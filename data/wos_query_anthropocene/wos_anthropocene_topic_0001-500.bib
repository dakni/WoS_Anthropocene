@article{ ISI:000426417200002,
Author = {Ochieng, Omedi},
Title = {{A SOCIAL MOVEMENT FOR THE ANTHROPOCENE: Wangari Maathai's African
   Feminist Environmentalist}},
Journal = {{AFRICAN JOURNAL OF RHETORIC}},
Year = {{2017}},
Volume = {{9}},
Number = {{1}},
Pages = {{15-29}},
Month = {{SEP}},
Abstract = {{In this paper, I explore Wangari Maathai's intersectional rhetoric of
   environmentalism, feminism, and good governance. I examine how Maathai
   crafted a response to the hegemonic forces of international and national
   capitalism, racism, and patriarchy. Maathai's rhetoric was by no means
   static. Her activism emerged from brilliant readings of the forces
   arrayed against her and often changed in response to the exigencies of
   time and space. I argue that a close rhetorical critique of the
   strategies that Maathai and her Green Belt Movement used in engaging a
   variety of allies and antagonists offers not only a comparative
   understanding of global rhetoric but also reveals the diversity and
   richness of African rhetorics. I aim to illuminate the insights and
   gains that emerged from Maathai's intersectional rhetoric even as I also
   proffer a critique of some of the debilitating assumptions and the blind
   spots of privilege that structured her activism.}},
