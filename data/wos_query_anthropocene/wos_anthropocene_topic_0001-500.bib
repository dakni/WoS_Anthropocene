@article{ ISI:000407683300020,
Author = {Bernhardt, Peter and Edens-Meier, Retha and Grimm, Wendy and Ren,
   Zong-Xin and Towle, Brian},
Title = {{GLOBAL COLLABORATIVE RESEARCH ON THE POLLINATION BIOLOGY OF RARE AND
   THREATENED ORCHID SPECIES (ORCHIDACEAE)}},
