@incollection{ ISI:000422709200010,
Author = {Shulmeister, James and Davies, Timothy R. H. and Bostock, Helen and
   Purdie, Heather and Nicol, Andrew and Shane, Philip A. and McGlone, Matt},
Editor = {{Shulmeister, J}},
Title = {{Adrift in the Anthropocene}},
Booktitle = {{LANDSCAPE AND QUATERNARY ENVIRONMENTAL CHANGE IN NEW ZEALAND}},
Series = {{Atlantis Advances in Quaternary Science}},
Year = {{2017}},
Volume = {{3}},
Pages = {{313-334}},
Abstract = {{This chapter examines likely future changes in New Zealand over the
   intermediate (100 year) and a longer term (5000 year) futures. The first
   section examines the underlying processes and concludes that in the
   longer term future, tectonic and volcanic processes are likely to have a
   stronger impact on New Zealand, as a landmass, than foreseeable future
   climate change. The risks to human activity from fault movement and
   volcanic eruptions are highlighted. The latter part of the chapter looks
   at likely geomorphic and biological changes. Geomorphic systems are more
   strongly modulated by tectonic processes but it is concluded that
   climate change will significantly impact glaciers and high mountain
   areas. These changes are largely negative but some new tourism
   opportunities may emerge. In contrast, the combination of climate
   change, increasing human pressure and geological disasters are
   cumulatively likely to significantly affect the remaining native biota
   of New Zealand. This is the most sensitive component of the New Zealand
   landscape and we are pessimistic about the prospects of maintaining
   native biodiversity.}},
