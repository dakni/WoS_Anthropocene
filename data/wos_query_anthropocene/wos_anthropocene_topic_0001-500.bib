@article{ ISI:000419312400007,
Author = {Davis, Heather and Todd, Zoe},
Title = {{On the Importance of a Date, or Decolonizing the Anthropocene}},
Journal = {{ACME-AN INTERNATIONAL E-JOURNAL FOR CRITICAL GEOGRAPHIES}},
Year = {{2017}},
Volume = {{16}},
Number = {{4}},
Pages = {{761-780}},
Abstract = {{This article argues for the importance of including Indigenous
   knowledges into contemporary discussions of the Anthropocene. We argue
   that a start date coincident with colonization of the Americas would
   more adequately open up these conversations. In this, we draw upon
   multiple Indigenous scholars who argue that the Anthropocene is not a
   new event, but is rather the continuation of practices of dispossession
   and genocide, coupled with a literal transformation of the environment,
   that have been at work for the last five hundred years. Further, the
   Anthropocene continues a logic of the universal which is structured to
   sever the relations between mind, body, and land. In dating the
   Anthropocene from the time of colonialization, the historical and
   ideological links between the events become obvious, providing a basis
   for the possibility of decolonization within this framework.}},
