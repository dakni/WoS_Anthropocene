@article{ ISI:000413507300004,
Author = {Davies, Kirsten and Adelman, Sam and Grear, Anna and Magallanes,
   Catherine Iorns and Kerns, Tom and Rajan, S. Ravi},
Title = {{The Declaration on Human Rights and Climate Change: a new legal tool for
   global policy change}},
