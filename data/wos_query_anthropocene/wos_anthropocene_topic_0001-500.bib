@article{ ISI:000412630000002,
Author = {Braidotti, Rosi},
Title = {{Posthuman Critical Theory}},
Journal = {{JOURNAL OF POSTHUMAN STUDIES-PHILOSOPHY TECHNOLOGY MEDIA}},
Year = {{2017}},
Volume = {{1}},
Number = {{1}},
Pages = {{9-25}},
Abstract = {{This article argues the case for posthuman critical theory within the
   context of the Anthropocene, as both the convergence of posthumanist and
   postanthropocentric discourses and their development in a qualitatively
   new and more complex direction. By adopting a cartographic approach to
   the posthuman, the article surveys recent scholarly production in the
   field and argues for the need to rethink subjectivity as a collective
   assemblage that encompasses human and nonhuman actors, technological
   mediation, animals, plants, and the planet as a whole. Resting on a
   monistic ontology drawn from critical Spinozism and Deleuzian vital
   materialism, the article makes the case for posthuman ethics as the
   expression of affirmative compositions of transversal, multiple, and
   collective practices of becoming-posthuman.}},
