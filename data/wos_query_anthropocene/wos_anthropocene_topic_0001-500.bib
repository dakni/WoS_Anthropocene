@article{ ISI:000394669700006,
Author = {Strydom, Piet},
Title = {{The sociocultural self-creation of a natural category:
   Social-theoretical reflections on human agency under the temporal
   conditions of the Anthropocene}},
Journal = {{EUROPEAN JOURNAL OF SOCIAL THEORY}},
Year = {{2017}},
Volume = {{20}},
Number = {{1, SI}},
Pages = {{61-79}},
Month = {{FEB}},
Abstract = {{Following the recent recognition that humans are an active force in
   nature that gave rise to a new geological epoch, this article explores
   the implications of the shift to the Anthropocene for social theory. The
   argument assumes that the emerging conditions compel an expansion and
   deepening of the timescale of the social-theoretical perspective and
   that such an enhancement has serious repercussions for the concept of
   human agency. First, the Anthropocene is conceptualized as a nascent
   cognitively structured cultural model rather than simply a geological
   epoch. Second, the vast and deep timescale, in the light of which the
   new time unit and its generative agency alone make sense, is analysed
   along with the human world's objective, sociocultural and subjective
   axes. Finally, the elements of the concept of agency are recomposed in
