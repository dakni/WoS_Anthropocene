@article{ ISI:000412437200006,
Author = {Martinez-Tilleria, Karina and Nunez-Avila, Mariela and Leon, Carolina A.
   and Pliscoff, Patricio and Squeo, Francisco A. and Armesto, Juan J.},
Title = {{A framework for the classification Chilean terrestrial ecosystems as a
   tool for achieving global conservation targets}},
