@article{ ISI:000424717200006,
Author = {Perez-Mendez, Nestor and Rodriguez, Airam and Nogales, Manuel},
Title = {{Intra-specific downsizing of frugivores affects seed germination of
   fleshy-fruited plant species}},
Journal = {{ACTA OECOLOGICA-INTERNATIONAL JOURNAL OF ECOLOGY}},
Year = {{2018}},
