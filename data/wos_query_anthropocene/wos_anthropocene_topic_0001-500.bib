@article{ ISI:000417926000002,
Author = {Lougheed, Scott Cameron and Hird, Myra J.},
Title = {{Food security and secure food in the Anthropocene}},
Journal = {{CRIME LAW AND SOCIAL CHANGE}},
Year = {{2017}},
