@article{ ISI:000394469300020,
Author = {Mussmann, S. M. and Douglas, M. R. and Anthonysamy, W. J. B. and Davis,
   M. A. and Simpson, S. A. and Louis, W. and Douglas, M. E.},
Title = {{Genetic rescue, the greater prairie chicken and the problem of
   conservation reliance in the Anthropocene}},
