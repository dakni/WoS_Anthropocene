@article{ ISI:000425746100001,
Author = {McGregor, Andrew and Houston, Donna},
Title = {{Cattle in the Anthropocene: Four propositions}},
Journal = {{TRANSACTIONS OF THE INSTITUTE OF BRITISH GEOGRAPHERS}},
Year = {{2018}},
