@incollection{ ISI:000424745900027,
Author = {Mayer, Sylvia},
Editor = {{Zapf, H}},
Title = {{World Risk Society and Ecoglobalism: Risk, Literature, and the
   Anthropocene}},
Booktitle = {{HANDBOOK OF ECOCRITICISM AND CULTURAL ECOLOGY}},
