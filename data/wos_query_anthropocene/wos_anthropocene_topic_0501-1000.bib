@article{ ISI:000382672200016,
Author = {Mix, Lucas John},
Title = {{LIFE-VALUE NARRATIVES AND THE IMPACT OF ASTROBIOLOGY ON CHRISTIAN ETHICS}},
Journal = {{ZYGON}},
