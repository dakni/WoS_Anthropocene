@article{ ISI:000367982900022,
Author = {Maheaux, Heather and Leavitt, Peter R. and Jackson, Leland J.},
Title = {{Asynchronous onset of eutrophication among shallow prairie lakes of the
   Northern Great Plains, Alberta, Canada}},
