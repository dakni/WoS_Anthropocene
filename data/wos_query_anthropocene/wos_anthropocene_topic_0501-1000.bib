@article{ ISI:000373537200011,
Author = {Fox, Coleen A. and Magilligan, Francis J. and Sneddon, Christopher S.},
Title = {{``You kill the dam, you are killing a part of me{''}: Dam removal and
   the environmental politics of river restoration}},
