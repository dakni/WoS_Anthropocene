@article{ ISI:000377359400011,
Author = {Burke, Anthony and Fishel, Stefanie and Mitchell, Audra and Dalby, Simon
   and Levine, Daniel J.},
Title = {{Planet Politics: A Manifesto from the End of IR}},
Journal = {{MILLENNIUM-JOURNAL OF INTERNATIONAL STUDIES}},
Year = {{2016}},
Volume = {{44}},
Number = {{3}},
Pages = {{499-523}},
