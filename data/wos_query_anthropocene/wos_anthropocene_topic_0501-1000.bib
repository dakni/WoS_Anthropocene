@article{ ISI:000372344700004,
Author = {Aguayo, Claudio and Higgins, Blanche and Field, Ellen and Nicholls,
   Jennifer and Pudin, Susan and Tiu, Sangion Appiee and Osborn, Maia and
   Hashemzadeh, Farshad and Lubuulwa, Kevin Kezabu and Boulet, Mark and
   Christie, Belinda A. and Mah, Jeremy},
Title = {{Perspectives From Emerging Researchers: What Next in EE/SE Research?}},
