@article{ ISI:000369117500009,
Author = {Brooke, John L. and Otter, Christopher},
Title = {{CONCLUDING REMARKS: THE ORGANIC ANTHROPOCENE}},
Journal = {{EIGHTEENTH-CENTURY STUDIES}},
Year = {{2016}},
