Title = {{The effects of human-mediated habitat fragmentation on a sedentary
   woodland-associated species (Rhinolophus hipposideros) at its range
   margin}},
Journal = {{ACTA CHIROPTEROLOGICA}},
Year = {{2016}},
Volume = {{18}},
Number = {{2}},
