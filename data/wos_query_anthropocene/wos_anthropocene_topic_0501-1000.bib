@article{ ISI:000377891200002,
Author = {Hoffman, William},
Title = {{Ecosystems, Food Crops, and Bioscience: A Symbiosis for the Anthropocene}},
Journal = {{ASIAN BIOTECHNOLOGY AND DEVELOPMENT REVIEW}},
