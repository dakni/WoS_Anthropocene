@article{ ISI:000382937900005,
Author = {Bird, Douglas W. and Bird, Rebecca Bliege and Codding, Brian F.},
Title = {{Pyrodiversity and the Anthropocene: the Role of Fire in the Broad
   Spectrum Revolution}},
