Author = {Habersack, Helmut and Haspel, Daniel and Kondolf, Mathias},
Title = {{Large Rivers in the Anthropocene: Insights and tools for understanding
   climatic, land use, and reservoir influences}},
Journal = {{WATER RESOURCES RESEARCH}},
