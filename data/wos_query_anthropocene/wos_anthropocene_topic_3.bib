@article{ ISI:000331149400002,
Author = {Voeroesmarty, Charles J. and Pahl-Wostl, Claudia and Bunn, Stuart E. and
   Lawford, Richard},
Title = {{Global water, the anthropocene and the transformation of a science}},
Journal = {{CURRENT OPINION IN ENVIRONMENTAL SUSTAINABILITY}},
