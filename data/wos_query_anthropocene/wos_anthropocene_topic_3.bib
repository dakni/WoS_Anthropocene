@article{ ISI:000356601900003,
Author = {South, Nigel},
Title = {{Anticipating the Anthropocene and greening criminology}},
Journal = {{CRIMINOLOGY \& CRIMINAL JUSTICE}},
Year = {{2015}},
Volume = {{15}},
