@article{ ISI:000356021700002,
Author = {Stevens, Carly J. and Lind, Eric M. and Hautier, Yann and Harpole, W.
   Stanley and Borer, Elizabeth T. and Hobbie, Sarah and Seabloom, Eric W.
   and Ladwig, Laura and Bakker, Jonathan D. and Chu, Chengjin and Collins,
