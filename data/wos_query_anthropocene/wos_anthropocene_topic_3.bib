@article{ ISI:000345532000032,
Author = {Shiue, Ivy and Samberg, Leah and Kulohoma, Benard and Dogaru, Diana and
   Wyborn, Carina and Hamel, Perrine and Jorgensen, Peter Sogaard and
   Lussier, Paul and Sundaram, Bharath and Lim, Michelle and Tironi,
   Antonio},
Title = {{2014 Future Earth Young Scientists Conference on Integrated Science and
   Knowledge Co-Production for Ecosystems and Human Well-Being}},
