Author = {Snell-Rood, Emilie and Cothran, Rickey and Espeset, Anne and Jeyasingh,
   Punidan and Hobbie, Sarah and Morehouse, Nathan I.},
Title = {{Life-history evolution in the anthropocene: effects of increasing
   nutrients on traits and trade-offs}},
Journal = {{EVOLUTIONARY APPLICATIONS}},
