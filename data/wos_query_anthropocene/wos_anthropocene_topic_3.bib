@article{ ISI:000358863500001,
Author = {Haidvogl, Gertrud and Hoffmann, Richard and Pont, Didier and Jungwirth,
   Mathias and Winiwarter, Verena},
Title = {{Historical ecology of riverine fish in Europe}},
