@article{ ISI:000421277600003,
Author = {Omelsky, Matthew},
Title = {{``After the End Times{''}: Postcrisis African Science Fiction}},
Journal = {{CAMBRIDGE JOURNAL OF POSTCOLONIAL LITERARY INQUIRY}},
