@article{ ISI:000329183200011,
Author = {Lewin, John and Macklin, Mark G.},
Title = {{Marking time in Geomorphology: should we try to formalise an
   Anthropocene definition?}},
Journal = {{EARTH SURFACE PROCESSES AND LANDFORMS}},
Year = {{2014}},
Volume = {{39}},
Number = {{1}},
Pages = {{133-137}},
Month = {{JAN}},
Abstract = {{The value of a formally defined Anthropocene for geomorphologists is
   discussed. Human impacts have been diachronistic, multifaceted and
   episodic, as demonstrated by the record of alluvial deposition in the
   UK. Rather than boxing time into discrete eras or periods, modern
   research uses calendar dates and multiple dating techniques to explore
   co-trajectories for a range of human impacts. Despite the value of The
   Anthropocene' as an informal concept and as a prompt to useful debate,
   arriving at a single, generally acceptable formal definition is
   impractical, and has some disadvantages. Copyright (c) 2013 John Wiley
   \& Sons, Ltd.}},
