@article{ ISI:000361205300003,
Author = {Hatvany, Matthew and Cayer, Donald and Parent, Alain},
Title = {{Interpreting Salt Marsh Dynamics: Challenging Scientific Paradigms}},
Journal = {{ANNALS OF THE ASSOCIATION OF AMERICAN GEOGRAPHERS}},
Year = {{2015}},
Volume = {{105}},
