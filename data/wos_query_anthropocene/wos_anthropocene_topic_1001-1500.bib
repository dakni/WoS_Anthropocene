@article{ ISI:000351773800015,
Author = {Zylinska, Joanna},
Title = {{ON LIFE, MOVEMENT AND STOPPAGE: AGENCY AND ETHICS IN THE ANTHROPOCENE}},
Journal = {{LEONARDO}},
Year = {{2015}},
Volume = {{48}},
Number = {{2}},
Pages = {{180-181}},
Month = {{APR}},
Abstract = {{Responding to the current narratives about the impending planetary
   catastrophe caused by our human activity, this philosophy-cum-art piece
   develops a more affirmative story about life, death and extinction.
   Framed as a non-normative ethics for the Anthropocene, it considers the
   human's expanded obligations towards the bio- and geosphere, while also
   critically reflecting on the very constitution of this human.}},
