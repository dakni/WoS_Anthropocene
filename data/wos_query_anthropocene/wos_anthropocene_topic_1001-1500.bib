@incollection{ ISI:000370448800025,
Author = {Viola, Eduardo and Basso, Larissa},
Editor = {{Rozzi, R and Chapin, FS and Callicott, JB and Pickett, STA and Power, ME and Armesto, JJ and May, RH}},
Title = {{Earth Stewardship, Climate Change, and Low Carbon Consciousness:
   Reflections from Brazil and South America}},
Booktitle = {{EARTH STEWARDSHIP: LINKING ECOLOGY AND ETHICS IN THEORY AND PRACTICE}},
Series = {{Ecology and Ethics}},
Year = {{2015}},
Volume = {{2}},
Pages = {{367-382}},
Abstract = {{The planet has entered the Anthropocene, a period in which humanity is
   the main driver of Earth systems changes. The effects of these fast and
   profound changes have been devastating for several fellow species -
   extinct or threatened to be extinct - and endanger a great part of the
   human population. In order to rebalance human interests and the
   equilibrium of the planet, Earth Stewardship, paradigm in which humanity
   recognizes itself as a co-inhabitant and steward of the planet, should
   be implemented. However, deep-rooted cultural traits can be stumbling
   blocks to the development of this new planetary ethics, and this is the
   case for South America. This chapter discusses the foundations of a new
   planetary ethics in South America and the challenges for its
   implementations, exemplified by one of the most contemporary complex
   issues, climate change.}},
