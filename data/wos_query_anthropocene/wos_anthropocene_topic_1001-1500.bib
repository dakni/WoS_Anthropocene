@incollection{ ISI:000370448800023,
Author = {Enkerlin-Hoeflich, Ernesto C. and Sandwith, Trevor and MacKinnon, Kathy
   and Allen, Diana and Andrade, Angela and Badman, Tim and Bueno, Paula
   and Campbell, Kathryn and Ervin, Jamison and Laffoley, Dan and Hay-Edie,
   Terence and Hockings, Marc and Johansson, Stig and Keenleyside, Karen
   and Langhammer, Penny and Mueller, Eduard and Vierros, Marjo and
   Welling, Leigh and Woodley, Stephen and Dudley, Nigel},
Editor = {{Rozzi, R and Chapin, FS and Callicott, JB and Pickett, STA and Power, ME and Armesto, JJ and May, RH}},
Title = {{IUCN/WCPA Protected Areas Program: Making Space for People and
   Biodiversity in the Anthropocene}},
Booktitle = {{EARTH STEWARDSHIP: LINKING ECOLOGY AND ETHICS IN THEORY AND PRACTICE}},
Series = {{Ecology and Ethics}},
Year = {{2015}},
Volume = {{2}},
Pages = {{339-350}},
Abstract = {{Protected areas have emerged as a cultural feature and perhaps the
   largest land resource allocation decision in human history. Yet they are
   not without controversy on their adequacy for conservation and social
   justice. We argue that protected areas not only are necessary for
   conservation, they also contribute to human well-being and social
   justice in the Anthropocene. The World Parks Congresses have been a
   major forum for advancing global protected area policy and practice.
   Recently the IUCN-World Commission on Protected Areas and the
   IUCN-Global Protected Areas Program has been moving toward a vision
   parallel and complementary to the proposed Earth Stewardship initiative
   of the Ecological Society of America. This novel view of IUCN is also
   called ``The Promise of Sydney{''} because it will be the focus of the
   2014 World Parks Congress in Australia. ICUN's novel view suggests that
   protected areas are an effective way to put Earth stewardship into
   action.}},
