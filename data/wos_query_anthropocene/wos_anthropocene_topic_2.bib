@article{ ISI:000377359400003,
Author = {Agathangelou, Anna M.},
Title = {{Bruno Latour and Ecology Politics: Poetics of Failure and Denial in IR}},
Journal = {{MILLENNIUM-JOURNAL OF INTERNATIONAL STUDIES}},
