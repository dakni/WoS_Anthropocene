@article{ ISI:000393202900002,
Author = {Clingerman, Forrest},
Title = {{Place and the Hermeneutics of the Anthropocene}},
Journal = {{WORLDVIEWS-GLOBAL RELIGIONS CULTURE AND ECOLOGY}},
Year = {{2016}},
Volume = {{20}},
Number = {{3, SI}},
Pages = {{225-237}},
Abstract = {{Connecting the Anthropocene to the ``spatial turn{''} in theology, the
   present essay has two goals. First, departing from more common readings,
   this essay suggests that ``the Anthropocene{''} is a hermeneutical
   concept, which serves as an interpretive name for how the contemporary
   human-environment relationship is materialized and thought. Second, this
   essay argues that the hermeneutics of the Anthropocene requires a point
   of mediation found in philosophical readings of human dwelling and the
   theological desire to find the integrity of place. The meaning of the
   Anthropocene can be used theologically to expose the contradictions
   within our imagination of the material location of the human future.}},
