Title = {{Late Quaternary vegetation and lake system dynamics in north-eastern
   Siberia: Implications for seasonal climate variability}},
Journal = {{QUATERNARY SCIENCE REVIEWS}},
Year = {{2016}},
Volume = {{147}},
Number = {{SI}},
