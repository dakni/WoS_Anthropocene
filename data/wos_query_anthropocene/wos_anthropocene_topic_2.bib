@article{ ISI:000390021600001,
Author = {Isenhour, Cindy and O'Reilly, Jessica and Yocum, Heather},
Title = {{Introduction to Special Theme Section ``Accounting for Climate Change:
   Measurement, Management, Morality, and Myth{''}}},
Journal = {{HUMAN ECOLOGY}},
Year = {{2016}},
Volume = {{44}},
Number = {{6}},
Pages = {{647-654}},
Month = {{DEC}},
Abstract = {{This special section considers contemporary efforts to account for
   climate change through four frames: measurement, management, morality
   and myth. Our introduction briefly outlines these perspectives and the
   relevant literature, asking: 1) How have techniques of measurement and
   quantification emerged from and contributed to the particular politics
   of the ``Anthropocene{''}?; 2) How have our efforts to measure
   socioclimatic systems facilitated new techniques of socio-environmental
   management and, at times, worked to reshape the very systems they
   describe?; 3) How have accounting practices worked to both elucidate and
   obscure questions of morality, value, responsibility and justice?; and
   4) How we might address the critique that climate science is a myth and
   improve understanding with greater incorporation of historical and cross
   cultural knowledge from human ecology, human geography and anthropology.}},
Publisher = {{SPRINGER/PLENUM PUBLISHERS}},
