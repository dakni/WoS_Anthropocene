@article{ ISI:000375733400003,
Author = {Parizeau, Marie-Helene},
Title = {{From the Apocalypse to the Anthropocene : ethical routes of climatic
   changes}},
Journal = {{REVUE DE METAPHYSIQUE ET DE MORALE}},
Year = {{2016}},
Number = {{1}},
Pages = {{23-37}},
Month = {{JAN}},
Abstract = {{Climate and politics are related since Aristotle. With Montesquieu, Ibn
   Khaldtln or Watsuji, climate determinism is attributed to the character
   of a nation. Since modernity shift, climate is then object of scientific
   knowledge. During the XX century, climate change has been documented
   even through controversies, and related to industrialization. The
   effects are a deadly threat to human beings and ecosystems. Climate
   ethics consider new relations to the biosphere or Gala. For some, in
   absence of any political agreements, disasters are inevitable. For
   others, Anthropocene by merging human history to natural history leads
   to technical action. The debate between climate determinism and human
   freedom is reopen.}},
