@article{ ISI:000381903800004,
Author = {Barton, C. Michael and Ullah, Isaac I. T. and Bergin, Sean M. and
   Sarjoughian, Hessam S. and Mayer, Gary R. and Bernabeu-Auban, Joan E.
   and Heimsath, Arjun M. and Acevedo, Miguel F. and Riel-Salvatore, Julien
