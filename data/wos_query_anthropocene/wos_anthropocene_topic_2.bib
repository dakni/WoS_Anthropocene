@article{ ISI:000393373800006,
Author = {Andersen, Gregers},
Title = {{The Destruction of Dwelling Ecopoetics in the Anthropocene}},
Journal = {{SGUARDO-RIVISTA DI FILOSOFIA}},
Year = {{2016}},
Number = {{21}},
Pages = {{77-90}},
Abstract = {{We are on the brink of entering a new age in Earth's history that calls
   for a radical rethinking of what Heidegger conceptualizes as dwelling.
   As the notion of the Anthropocene makes it ontologically impossible for
   humans to let non-human entities appear in their own presencing,
   Heidegger's fourfold dissolves and a new geomorphic fold emerges as the
   ground for human existence. In this article I trace how this fold
   transfigures the relationship between poetics and ecological engagement,
   and thereby constitute a new operating space for contemporary
   literature.}},
