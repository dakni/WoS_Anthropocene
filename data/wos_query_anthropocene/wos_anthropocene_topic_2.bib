Title = {{On-site data cast doubts on the hypothesis of shifting cultivation in
   the late Neolithic (c. 4300-2400 cal. BC): Landscape management as an
   alternative paradigm}},
Journal = {{HOLOCENE}},
Year = {{2016}},
Volume = {{26}},
