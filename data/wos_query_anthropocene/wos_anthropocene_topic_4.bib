@article{ ISI:000187988000003,
Author = {Meybeck, M},
Title = {{Global analysis of river systems: from Earth system controls to
   Anthropocene syndromes}},
Journal = {{PHILOSOPHICAL TRANSACTIONS OF THE ROYAL SOCIETY B-BIOLOGICAL SCIENCES}},
Year = {{2003}},
