@article{ ISI:000209235200006,
Author = {Kim, Rakhyun E. and Bosselmann, Klaus},
Title = {{International Environmental Law in the Anthropocene: Towards a Purposive
   System of Multilateral Environmental Agreements}},
Journal = {{TRANSNATIONAL ENVIRONMENTAL LAW}},
