@article{ ISI:000397578900022,
Author = {Conway, Tenley M.},
Title = {{Conceptual pathways and institutional barriers for urban physical
   geography: A response to Ashmore and Dodson}},
Journal = {{CANADIAN GEOGRAPHER-GEOGRAPHE CANADIEN}},
Year = {{2017}},
Volume = {{61}},
Number = {{1, SI}},
Pages = {{107-111}},
Month = {{SPR}},
Abstract = {{As suggested by Ashmore and Dodson (2016), emphasis on urban physical
   geography would contribute to deeper understandings about the
   Anthropocene, while supporting the management of more livable cities and
   suburbs. Urban physical geography can look to urban ecology for
   conceptual models that will support exploration of urban areas as unique
   socio-ecological systems. There are, however, institutional barriers to
   achieving a more robust urban physical geography in Canada. These
   include the siloed structure of the main funding bodies and traditional
   separation of human and physical geography in undergraduate programs.
   While overcoming these barriers is not easy, urban physical geography
   has the potential to provide critical insights into the places where
   most Canadians live and work.}},
