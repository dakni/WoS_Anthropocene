@article{ ISI:000412818400002,
Author = {Roberts, Tom},
Title = {{Thinking technology for the Anthropocene: encountering 3D printing
   through the philosophy of Gilbert Simondon}},
Journal = {{CULTURAL GEOGRAPHIES}},
