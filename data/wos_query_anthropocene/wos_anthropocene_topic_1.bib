@article{ ISI:000412308500004,
Author = {Smith-Nonini, Sandy},
Title = {{Making Complexity Your Friend: Reframing Social Theory for the
   Anthropocene}},
Journal = {{WEATHER CLIMATE AND SOCIETY}},
