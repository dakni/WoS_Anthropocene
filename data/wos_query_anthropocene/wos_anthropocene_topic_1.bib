@article{ ISI:000401706500004,
Author = {Smith, Thomas S. J.},
Title = {{Of Makerspaces and Hacklabs: Emergence, Experiment and Ontological
   Theatre at the Edinburgh Hacklab, Scotland}},
Journal = {{SCOTTISH GEOGRAPHICAL JOURNAL}},
Year = {{2017}},
Volume = {{133}},
Number = {{2}},
Pages = {{130-154}},
Abstract = {{This paper commences a geographical engagement with makerspaces,
   hacklabs, and other workshop spaces which form part of a broader maker
   movement'. It examines the arts of inquiry and experimentation found at
   one such site, drawing on ethnographic field work at the Edinburgh
   Hacklab, and makes connections with emerging themes of interest to
   geographers, including creativity, experiment, art, and nonhuman agency.
   Putting standard innovation-driven narratives of makerspaces into
   question, attention is instead turned to the events of emergent
   experimentation and creativity taking place in these spaces. To this
