@article{ ISI:000408861800046,
Author = {Heurtebise, Jean-Yves},
Title = {{Sustainability and Ecological Civilization in the Age of Anthropocene:
   An Epistemological Analysis of the Psychosocial and ``Culturalist{''}
   Interpretations of Global Environmental Risks}},
Journal = {{SUSTAINABILITY}},
Year = {{2017}},
Volume = {{9}},
Number = {{8}},
Month = {{AUG}},
Abstract = {{The aim of this article is to assess the validity of the culturalist
   explanation of unsustainability by critically examining the
   social-cultural interpretation of the risks on which it is
   epistemologically based. First, we will explore the different ways in
   which the notion of Anthropocene is changing our perception of risks.
   Second, we will analyze the limits of the social-cultural explanation of
   risks relative to the global (non-linear) interdependence between human
   activities and environmental processes that defines the Anthropocene.
   Third, we will introduce the Chinese concept of Ecological Civilization
   and analyze its cultural foundations and culturalist assumptions.
   Finally, we will develop the practical consequences of this critic of
   the social-cultural interpretation of risks and of culturalist
   explanations of unsustainability.}},
