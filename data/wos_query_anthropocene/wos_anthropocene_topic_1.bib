@article{ ISI:000397029000002,
Author = {Hoffmann, Goesta},
Title = {{Why the Anthropocene is unsuitable as a new geochronological epoch}},
Journal = {{ZEITSCHRIFT DER DEUTSCHEN GESELLSCHAFT FUR GEOWISSENSCHAFTEN}},
Year = {{2017}},
Volume = {{168}},
Number = {{1}},
Pages = {{3-7}},
Month = {{MAR}},
Abstract = {{The Anthropocene concept is critically discussed in terms of its
   stratigraphic significance as well as its geological relevance. The
   Holocene is an interglacial in the current ice age and differs from
   previous ones due to the presence of Homo sapiens. Indeed, the latter
   part of the Holocene can be considered to be dominated by human-induced
   global environmental changes. Despite this, the introduction of a new
   stratigraphic unit, with the status of an epoch and characterised by the
   influence of humans, is unnecessary.}},
